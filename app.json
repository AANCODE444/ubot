{
    "name": "Geez Pyro Userbot",
    "description": "UserBot BIASA AJAH !!!.",
    "logo": "https://telegra.ph/file/c78bb1efdeed38ee16eb2.png",
    "keywords": ["telegram", "userbot", "Geez", "pyrogram"],
    "repository": "https://github.com/hitokizzy/Geez-Pyro",
    "stack": "container",
    "env": {
        "STRING_SESSION1": {
            "description": "Ambil string pyrogram dari https://t.me/geezramstringbot",
            "required": true
        },
        "MONGO_URL": {
            "description": "mongodb.com Database",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Sudo list (user id)",
            "required": false
        },
        "OWNER_ID": {
            "description": "id telegram anda",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "bot token dari @BotFather telegram",
            "required": true,
            "value": ""
        },
        "HEROU_APP_NAME": {
            "description": "Heroku app name",
            "required": true,
            "value": ""
        },
        "HEROU_API_KEY": {
            "description": "Heroku app key",
            "required": true,
            "value": ""
        },
        "BOTLOG_CHATID": {
            "description": "Group publik untuk log botnya",
            "required": true,
            "value": "-100"
        },
        "buildpacks": [
          {
              "url": "heroku/python"
          },
          {
              "url": "heroku/nodejs"
          },
          {
              "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
          }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "basic"
        },
    "addons": [{
        "plan": "heroku-postgresql",
        "options": {
            "version": "12"
        }
        }
    ]
    }
  }
}
